<!DOCTYPE html>

<title>Evaluation</title>

{% block body %}
    <h1>Evaluation</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ evaluation.id }}</td>
            </tr>
            <tr>
                <th>Nom_client</th>
                <td>{{ evaluation.nomClient }}</td>
            </tr>
            <tr>
                <th>Prenom_client</th>
                <td>{{ evaluation.prenomClient }}</td>
            </tr>
            <tr>
                <th>Avis_client</th>
                <td>{{ evaluation.avisClient }}</td>
            </tr>
            <tr>
                <th>Date_eval</th>
                <td>{{ evaluation.dateEval ? evaluation.dateEval|date('Y-m-d') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_evaluation_index') }}">back to list</a>

    <a href="{{ path('app_evaluation_edit', {'id': evaluation.id}) }}">edit</a>

    {{ include('evaluation/_delete_form.html.twig') }}
{% endblock %}
