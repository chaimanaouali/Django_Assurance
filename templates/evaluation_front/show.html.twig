{% extends 'client/base-client.html.twig' %}

{% block body %}
    <h1>Evaluation</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ evaluation.id }}</td>
            </tr>
            <tr>
                <th>Nom_client</th>
                <td>{{ evaluation.nomClient }}</td>
            </tr>
            <tr>
                <th>Prenom_client</th>
                <td>{{ evaluation.prenomClient }}</td>
            </tr>
            <tr>
                <th>Avis_client</th>
