{% extends 'admin/base-front.html.twig' %}
{% block title %} devis{% endblock %}

{% block body %}
    <!DOCTYPE html>
    <h1>Devis</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ devi.id }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ devi.nom }}</td>
            </tr>
            <tr>
                <th>Prenom</th>
                <td>{{ devi.prenom }}</td>
            </tr>
            <tr>
                <th>Adresse</th>
                <td>{{ devi.adresse }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ devi.email }}</td>
            </tr>
            <tr>
                <th>Date_naiss</th>
                <td>{{ devi.dateNaiss ? devi.dateNaiss|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>Num_tel</th>
                <td>{{ devi.numTel }}</td>
            </tr>
            <tr>
                <th>Modele</th>
                <td>{{ devi.modele }}</td>
            </tr>
            <tr>
                <th>Puissance</th>
                <td>{{ devi.puissance }}</td>
            </tr>
            <tr>
                <th>Prix</th>
                <td>{{ devi.prix }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_devis_index') }}" class="btn btn-primary">Back to List</a>

    <a href="{{ path('app_devis_edit', {'id': devi.id}) }}" class="btn btn-warning">Edit</a>

    {{ include('devis/_delete_form.html.twig') }}
{% endblock %}
